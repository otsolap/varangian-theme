        "blocks.blogs",
        "blocks.cards",
        "blocks.hero",
        "blocks.banner",
        "blocks.media-mix",
        "blocks.text-area",
        "blocks.form-embed",
        "blocks.video-embed",
        "blocks.image-block",
        "blocks.image-gallery",
        "blocks.quote",
        "blocks.services"
      ]
    },
    "showServicesBanner": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "showSubscriptionForm": {
      "type": "boolean",
      "default": false
    }
  }
