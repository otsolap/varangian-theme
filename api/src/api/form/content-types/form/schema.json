{
  "kind": "collectionType",
  "collectionName": "forms",
  "info": {
    "singularName": "form",
    "pluralName": "forms",
    "displayName": "Form",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "inputs": {
      "type": "dynamiczone",
      "components": [
        "form-inputs.text-field",
        "form-inputs.textarea-field",
        "form-inputs.email"
      ]
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "button": {
      "type": "component",
      "repeatable": false,
      "component": "partials.button"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "showLabels": {
      "type": "boolean",
      "default": false
    },
    "formID": {
      "type": "uid",
      "required": true
    },
    "contact": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::contact.contact",
      "mappedBy": "form"
    }
  }
}
